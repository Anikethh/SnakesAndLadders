              preserveRatio="true">
      <Image url="@sample/Images/winner-01.png"/>
   </ImageView>
   <Button fx:id="replay" layoutX="120.0" layoutY="407.0" mnemonicParsing="false" onAction="#winnertogame" opacity="0.0"
           prefHeight="36.0" prefWidth="96.0" text="Replay"/>
   <ImageView fitHeight="36.0" fitWidth="120.0" layoutX="119.0" layoutY="486.0" pickOnBounds="true"
              preserveRatio="true">
      <Image url="@sample/Images/menu-02.png"/>
   </ImageView>
   <Button fx:id="backButton" layoutX="124.0" layoutY="486.0" mnemonicParsing="false" onAction="#playerToCover" opacity="0.0"
           prefHeight="36.0" prefWidth="87.0" text="backk"/>
</AnchorPane>
