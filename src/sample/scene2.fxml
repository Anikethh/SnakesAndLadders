      <effect>
         <Glow />
      </effect></Button>
   <TextField fx:id="player1" layoutX="142.0" layoutY="270.0" prefHeight="26.0" prefWidth="130.0" promptText="Player1" />
   <TextField fx:id="player2" layoutX="143.0" layoutY="302.0" prefHeight="26.0" prefWidth="129.0" promptText="Player2" />
</AnchorPane>
